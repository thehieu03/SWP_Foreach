<?xml version="1.0" encoding="UTF-8"?>
<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.migrateproject.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadAutomaker</servlet-name>
        <servlet-class>com.example.migrateproject.controller.LoadAutomaker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>requestData</servlet-name>
        <servlet-class>com.example.migrateproject.controller.RequestData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataRequestSeverlet</servlet-name>
        <servlet-class>com.example.migrateproject.controller.DataRequestSeverlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadAutomaker</servlet-name>
        <url-pattern>/LoadAutomaker</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.example.migrateproject.controller.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataRequestSeverlet</servlet-name>
        <url-pattern>/DataRequestSeverlet</url-pattern>
    </servlet-mapping>
</web-app>
